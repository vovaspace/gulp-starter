{
  "example": {
    "title": "Gulp Starter",
    "sections": [
      {
        "title": "Clone, install dependencies, <br>build and serve on localhost:3000",
        "commands": [
          "git clone https://github.com/vovaspace/gulp-starter.git",
          "cd gulp-starter",
          "rm -rf .git",
          "npm i",
          "gulp"
        ],
        "isWide": true
      },
      {
        "title": "Build",
        "commands": [
          "gulp build"
        ],
        "isWide": false
      },
      {
        "title": "Deploy /build on GitHub Pages",
        "commands": [
          "gulp deploy"
        ],
        "isWide": false
      }
    ]
  }
}